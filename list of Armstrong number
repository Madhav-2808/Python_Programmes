def armstrong_range():
    a=int(input('enter the lower limit (2 digit):'))
    b=int(input('enter the upper limit :'))
    for num in range(a, b + 1):
        order = len(str(num))
        sum = 0
        temp = num
        while temp > 0:
            digit = temp % 10
            sum += digit ** order
            temp //= 10
        if num == sum:
            print(num)
armstrong_range()
